    <title>Insert title here</title>
    <script type ="text/javascript">
        var a = 8;
        if(a % 2 == 0){
            document.write("짝수: " +a + "<br>");
        }else{
            document.write("홀수: " +a + "<br>");
        }

        var c = 7;
        if(c % 2 ==0){
            document.write("2의 배수: " +c + "<br>");
        }else if(c % 3 ==0){
            document.write("3의 배수: " +c + "<br>");
        }else{
            document.write("기타: " +c + "<br>");
        }
        //for 문 1~100 까지의 합
        //int sum=0;
        //for(int i = 0;i<=100;i++){}

        var sum =0;
        for(var i =0; i<=100;i++){
            sum +=i;
        }
        document.write(sum);

        for(var i =2; i<10;i++){
            for(var j = 1; j <10 ; j++){
                document.write(i +"*"+j+ "="+i*j + "<br>");
            }
        }
        

    </script>
</head>
