package kr.or.bit;

import java.util.Arrays;

public class Ppt5_2 {
	int[] a = {45,20,63,2,71,38,86,99,12,54};
	
	public void showA() {
		System.out.println("¹è¿­ aArray =" + Arrays.toString(a));
	}
	
	public void minA() {
		
	}
	
	public void maxA() {
		
	}
	
	public void lineA() {
		for (int i = 0; i < a.length; i++) {
			for (int j = i + 1; j < a.length; j++) {
				if (a[i] > a[j]) {
					int temp = a[i];
					a[i] = a[j];
					a[j] = temp;
					//return Arrays.toString(a);
				}
			}
		}
		
	}


}
