package kr.or.bit;

public class Ppt5_3 {
	int[] answer = {1,4,4,3,1,4,4,2,1,3,2};
	
	public void starAnswer() {
		for(int i = 0; i <answer.length; i++) {
			if(answer[i] == 1) {
				System.out.print("*");
				System.out.println();
			}else if(answer[i] == 2) {
				System.out.print("*");
				System.out.println();
			}else if(answer[i] == 3) {
				System.out.print("*");
				System.out.println();
			}else {
				System.out.print("*");
				System.out.println();
			}
		}
	}
	
	

}
